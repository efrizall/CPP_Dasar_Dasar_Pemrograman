#include<iostream>
using namespace std;

int main(){
	int dat,n;
	cin>>n;
	bool ketemu = true;
	//bool tdk_ketemu = false;
	int arr[10] = {1,2,3,4,6,8,11,12,15,17};
	for(int i=0; i<10; i++){
		if (arr[i]==n){
			dat++;
			cout<< "Angka Ketemu : "<< dat; 
			}
		//else {
		//	tdk_ketemu = false;
		//	cout<< "Angka Tidak Ditemukan : "<< i;
